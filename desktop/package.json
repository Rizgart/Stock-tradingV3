{
  "name": "stock-trading-desktop",
  "version": "0.1.0",
  "description": "Electron + React desktop app",
  "main": "electron-main.js",
  "scripts": {
    "start": "concurrently \"npm:serve-react\" \"npm:serve-electron\"",
    "serve-react": "vite",
    "serve-electron": "wait-on tcp:5173 && electron .",
    "build": "vite build && electron-builder",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest run"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "axios": "^1.6.8",
    "electron-log": "^5.1.4",
    "i18next": "^23.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vitest": "^1.5.0",
    "wait-on": "^7.2.0"
  }
}
